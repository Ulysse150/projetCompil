var point p;
var point t;
var int a;

var int b;
var bool bo;

class point {
  attribute int x;
  attribute int y;

  method void constructor(int a, int b) {
    this.x = a;
    this.y = b;
    a = 78;
  }

  method void double(){
    this.x = this.x + this.x;
    this.y = this.y + this.y;
  }

  method int somme() {
   
    
    return this.x + this.y;
  }

  method int facto( int n){
   
    if (n == 0){
      return 1;
    }else{
      return n * this.facto(n-1);
    }
  }



}


main {
  a = 7;
 
  p = new point(2,1);
  
  t = new point(4, 3);

  print(p.somme());
  bo = p =/= t;
  if (bo){
    print(7);
  }else{
    print(4);
  }
  
 
  
}
