class paire {
  attribute int x;
  attribute int y;

  method void constructor(int x, int y) {
    this.x = x;
    this.y = y;
  }

  method int somme() {

    
    return this.x + this.y  ;
    

  }

  method void augmente(){
    this.x = this.x * 2;
    this.y = 2 * this.y;
  }

}

class triple extends paire {
  attribute int z;

  method void constructor(int x, int y, int z) {
   
    this.x = x;
    this.y = y;
    print(4);
    this.z = z;
  }

  method int somme() {
    print(pi);
    return this.x + this.y + this.z;
  }

  method int produit() {
    return this.x * this.y * this.z;
  }
}

